        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {

    var agg_op = new GlideAggregate('x_200145_trade_sa_operation');
    agg_op.addQuery('day_trade', current.day_trade);
    agg_op.addQuery('result', 'gain');
    agg_op.addAggregate("COUNT");
    agg_op.query();

    var rate = 0;

    if (agg_op.next()) {

        rate = agg_op.getAggregate("COUNT");

    }

    if (rate != 0) {

        var agg_op_tt = new GlideAggregate('x_200145_trade_sa_operation');
        agg_op_tt.addQuery('day_trade', current.day_trade);
        agg_op_tt.addAggregate("COUNT");
        agg_op_tt.query();

        if (agg_op_tt.next()) {

            var total = agg_op_tt.getAggregate("COUNT");
			
            rate = rate / total * 100;           

        }

    }
	
	var gr_dt = current.day_trade.getRefRecord();
	gr_dt.efficiency_rate = rate;
	gr_dt.update();

})(current, previous);]]></script>
